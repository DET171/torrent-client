{
  "name": "torrent-client",
  "version": "0.1.0",
  "main": "public/electron.js",
  "license": "MIT",
  "homepage": "./",
  "description": "A torrent client which uses webtorrent",
  "private": true,
  "dependencies": {
    "@craco/craco": "^6.4.5",
    "@fortawesome/fontawesome-free": "^6.2.0",
    "@reforged/maker-appimage": "^1.2.1",
    "@semantic-ui-react/css-patch": "^1.1.2",
    "@szmarczak/http-timer": "^5.0.1",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^13.0.0",
    "@testing-library/user-event": "^13.2.1",
    "asar": "^3.2.0",
    "downloads-folder": "^3.0.3",
    "electron-is-dev": "^2.0.0",
    "electron-reload": "^2.0.0-alpha.1",
    "electron-squirrel-startup": "^1.0.0",
    "got": "^12.5.1",
    "moment": "^2.29.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-scripts": "5.0.1",
    "semantic-ui-css": "^2.4.1",
    "semantic-ui-react": "^2.1.3",
    "web-vitals": "^2.1.0",
    "webtorrent": "^1.8.30",
    "webtorrent-hybrid": "^5.0.2"
  },
  "scripts": {
    "electron:start": "electron .",
    "postinstall": "semantic-ui-css-patch",
    "craco:start": "craco start",
    "lint": "eslint --ext .js,.jsx,.ts,.tsx .",
    "craco:build": "craco build",
    "start": "electron-forge start",
    "package": "electron-forge package",
		"dist": "electron-builder",
    "make": "electron-forge make"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@electron-forge/cli": "^6.0.0-beta.66",
    "@electron-forge/maker-deb": "^6.0.0-beta.66",
    "@electron-forge/maker-rpm": "^6.0.0-beta.66",
    "@electron-forge/maker-squirrel": "^6.0.0-beta.66",
    "@electron-forge/maker-zip": "^6.0.0-beta.66",
    "@electron/remote": "^2.0.8",
    "autoprefixer": "^10.4.12",
    "concurrently": "^7.4.0",
    "electron": "^21.0.1",
    "electron-builder": "^23.3.3",
    "eslint": ">=5.16.0",
    "eslint-config-berry": "^0.15.5",
    "eslint-config-google": "^0.14.0",
    "eslint-plugin-react": "^7.31.8",
    "postcss": "^8.4.17",
    "tailwindcss": "^3.1.8",
    "wait-on": "^6.0.1",
    "webpack-node-externals": "^3.0.0"
  },
  "build": {
    "appId": "com.electron.torrent-client",
    "productName": "Torrent Client",
    "copyright": "Copyright Â© 2022 mrgeek484@gmail.com"
  },
  "config": {
    "forge": {
      "packagerConfig": {
        "extraResources": [
          "./node_modules/@electron/remote/**"
        ]
      },
      "makers": [
        {
          "name": "@reforged/maker-appimage"
        },
        {
          "name": "@electron-forge/maker-zip",
          "platforms": [
            "darwin",
            "linux"
          ]
        },
        {
          "name": "@electron-forge/maker-deb",
          "config": {}
        }
      ]
    }
  }
}
